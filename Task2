public class Task3 {
	public static void main(String[] args) {
		int x = 2880568, sum = 0;
		while (x != 2147483647) {
			for (int i = 1000000000; i >= 1; i /= 10) {
				sum += (int) (x / i);
				x -= (int) (x / i) * i;
				if (sum == 0) {
				} else if (sum >= 1) {
					System.out.println(sum);
				}
			}
			break;
		}
	}
}
