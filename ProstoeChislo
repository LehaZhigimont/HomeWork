//Простые числа <N
public class Task3 {
	public static void main(String[] args) {
		int N = 10;
		boolean etoChisloNeprostoe = false;
		System.out.println("Простые чиста <= "+ N+ " :");
		while (N > 1) {
			for (int i = 2; i < N; i++) {
				if (N % i == 0) {
					etoChisloNeprostoe = false;
					break;
				} else
					etoChisloNeprostoe = true;
			}
			if (etoChisloNeprostoe == true)
				System.out.print(N + " ");
			N--;
		}
	}
}
